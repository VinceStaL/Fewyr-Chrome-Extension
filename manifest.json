{
  "manifest_version": 3,
  "name": "Fewyr - clear agendas, fewer meetings",
  "version": "1.0.1",
  "description": "Create clear meeting agendas with fewer meetings",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Open Fewyr"
  },
  "permissions": [
    "clipboardWrite",
    "storage",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}